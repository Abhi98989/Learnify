    <!-- JavaScript to handle the FAQ toggle behavior -->
    <script>
        document.addEventListener('DOMContentLoaded', function() {
          const faqItems = document.querySelectorAll('.faq-item');

          faqItems.forEach(item => {
            const toggleIcon = item.querySelector('.faq-toggle');
            const faqContent = item.querySelector('.faq-content');

            item.querySelector('h3').addEventListener('click', () => {
              const isActive = item.classList.contains('faq-active');
              item.classList.toggle('faq-active', !isActive);
              toggleIcon.textContent = isActive ? '' : ''; 
            });
          });
        });
    </script>
